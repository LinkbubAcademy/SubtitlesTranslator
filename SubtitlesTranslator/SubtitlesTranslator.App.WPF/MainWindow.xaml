<Window x:Class="SubtitlesTranslator.App.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SubtitlesTranslator.App.WPF"
        xmlns:uc="clr-namespace:SubtitlesTranslator.App.WPF.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="840" Width="1200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <uc:ImportView x:Name="ImportView" Grid.Column="0" Margin="5,5,0,5"></uc:ImportView>
        <uc:EdtiView x:Name="EditView" Grid.Column="1" Margin="0,5,5,5"></uc:EdtiView>
        
    </Grid>
</Window>
